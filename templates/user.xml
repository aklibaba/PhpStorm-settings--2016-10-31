<templateSet group="user">
  <template name="comment-section" value="/* ======================================================================&#10;   $comment$&#10;   ====================================================================== */&#10;   $END$" description="Section comment block" toReformat="false" toShortenFQNames="true">
    <variable name="comment" expression="" defaultValue="&quot;Section comment block&quot;" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="comment-sub-section" value="/* $comment$&#10;   ======================================================== */&#10;   $END$" description="Section comment block" toReformat="false" toShortenFQNames="true">
    <variable name="comment" expression="" defaultValue="&quot;Section comment block&quot;" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="print-path" value="&lt;?php print $path ?&gt;" description="PHP print path" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="conlog" value="console.log($END$);" description="Console Log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>